{
  "author": "Ben Saufley <contact@bensaufley.com>",
  "dependencies": {
    "firebase": "^7.14.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2"
  },
  "description": "A place to play games through the Internet",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@types/html-webpack-plugin": "^3.2.2",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.2",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-test-renderer": "^16.9.2",
    "@types/webpack": "^4.41.12",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "babel-jest": "^25.4.0",
    "babel-loader": "^8.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-airbnb-typescript-prettier": "^2.1.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "eslint-plugin-simple-import-sort": "^5.0.2",
    "firebase-admin": "^8.11.0",
    "firebase-functions": "^3.6.1",
    "firebase-functions-test": "^0.2.1",
    "firebase-tools": "^8.2.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.2.0",
    "jest": "^25.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "ts-node": "^8.9.1",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "engines": {
    "node": "10"
  },
  "license": "MIT",
  "main": ".build/index.js",
  "name": "bnsfly-games",
  "repository": "https://github.com/bensaufley/games",
  "scripts": {
    "build": "npm-run-all -p build:functions build:site",
    "build:functions": "cd functions && tsc",
    "build:site": "rimraf build/assets && webpack --mode production",
    "deploy:functions": "firebase deploy --only functions",
    "format": "npm run lint -- --fix",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "logs:functions": "firebase functions:log",
    "serve": "npm-run-all serve:site serve:emulators",
    "serve:site": "firebase serve --port 7787",
    "serve:emulators": "sleep 1 && firebase emulators:start",
    "shell:functions": "yarn build:functions && firebase functions:shell",
    "start": "npm-run-all -p 'build:functions --watch' 'build:site --watch' serve",
    "test": "jest"
  },
  "version": "0.1.0"
}
